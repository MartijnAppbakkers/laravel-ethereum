            "Appbakkers\\Ethereum\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appbakkers\\Ethereum\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appbakkers\\Ethereum\\EthereumServiceProvider"
            ]
        }
    }
}
